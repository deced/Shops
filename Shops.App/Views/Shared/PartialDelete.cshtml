<div class="modal" tabindex="-1" id="dialogDelete" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Confirmation</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <input hidden id="deleteId">
                <p>Are you sure you want to delete?</p>
            </div>
            <div class="modal-footer">
                <button type="button" onclick="deleteModel()" class="btn btn-danger" data-dismiss="modal">Delete</button>
                <button type="button" data-toggle="modal" data-target="#dialogDelete" class="btn btn-primary">Cancel</button>
            </div>
        </div>
    </div>
</div>