<div class="modal" tabindex="-1" id="dialogConfirm" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Confirmation</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <input hidden id="confirmId">
                <p>Are you sure you want to confirm?</p>
            </div>
            <div class="modal-footer">
                <button type="button" onclick="confirmModel()" class="btn btn-danger" data-dismiss="modal">Confirm</button>
                <button type="button" data-toggle="modal" data-target="#dialogConfirm" class="btn btn-primary">Cancel</button>
            </div>
        </div>
    </div>
</div>